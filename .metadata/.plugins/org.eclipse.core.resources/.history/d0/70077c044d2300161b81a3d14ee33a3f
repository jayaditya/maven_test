select message,EXCEPTION from exc_msg_exception where SYS_INSERT_TSTMP between '05-APR-16 12.00.00.000000 AM' and '05-APR-16 11.59.59.000000 PM'  and QUEUE_NAME='INT.CME.AUDITING';

SELECT EXCEPTION_ID,REPLAY_STATUS,REPLAY_COUNT,SYS_INSERT_TSTMP FROM EXC_MSG_eXCEPTION WHERE SYS_INSERT_TSTMP between '18-DEC-15 04.00.00.000000 PM' and'18-DEC-15 04.59.59.000000 PM'  and QUEUE_NAME='STERLING_MS.CUS011_DC1_SYW1_V001.00.00.PPQ';

SELECT EXCEPTION_ID,REPLAY_STATUS,REPLAY_COUNT,SYS_INSERT_TSTMP FROM EXC_MSG_eXCEPTION WHERE  EXCEPTION_ID in (15699566,15699565);

Set pagesize 100
set linesize 500
select count(*),queue_name, max(SYS_INSERT_TSTMP), mIN(SYS_INSERT_TSTMP)
from exc_msg_exception
where SYS_INSERT_TSTMP between '11-APR-16 12.00.00.000000 AM' and '11-APR-16 11.59.59.000000 PM' group by queue_name order by 3,2;

Set pagesize 100
set linesize 500
select count(*),queue_name, max(SYS_INSERT_TSTMP), mIN(SYS_INSERT_TSTMP),ERROR_CODE,replay_status
from exc_msg_exception
where SYS_INSERT_TSTMP between '24-MAR-16 12.00.00.000000 AM' and '28-MAR-16 11.59.59.000000 PM' and queue_name like '%CUS%' group by queue_name,ERROR_CODE,replay_status order by 3,2;

select message,exception from EXC_MSG_eXCEPTION WHERE MESSAGE LIKE '%306-0003541-7932138%';

