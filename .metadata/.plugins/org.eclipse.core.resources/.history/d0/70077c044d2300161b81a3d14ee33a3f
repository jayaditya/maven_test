select MSG_UNQ_ID,intrfc_prtnr_sys_id,SYS_INSERT_TSTMP,CME_INTRFC_ID,msg_direction from txn_cme_audit_hist where msg_unq_id like '%301-0003560-4399257%' and msg_unq_id like '%GIFTCARD_B2C%';


SELECT  COUNT (*),TO_CHAR(TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS')  FROM TXN_CME_AUDIT WHERE TXN_CME_AUDIT.SYS_INSERT_TSTMP > TO_DATE ('24-DEC-2015', 'dd - mon- yyyy')AND TXN_CME_AUDIT.SYS_INSERT_TSTMP < TO_DATE ('29-DEC-2015','dd - mon - yyyy')  AND msg_direction='O' GROUP BY TO_CHAR(TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS')ORDER BY TO_CHAR (TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS')ASC;


SELECT  COUNT (*),INTRFC_PRTNR_SYS_ID,CME_INTRFC_ID,MSG_DIRECTION,TO_CHAR(TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS')  FROM TXN_CME_AUDIT WHERE TXN_CME_AUDIT.SYS_INSERT_TSTMP > TO_DATE ('28-DEC-2015', 'dd - mon- yyyy')AND TXN_CME_AUDIT.SYS_INSERT_TSTMP < TO_DATE ('29-DEC-2015','dd - mon - yyyy') AND msg_direction='O' GROUP BY TO_CHAR(TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS'),INTRFC_PRTNR_SYS_ID,CME_INTRFC_ID,MSG_DIRECTION ORDER BY TO_CHAR (TRUNC (TXN_CME_AUDIT.SYS_INSERT_TSTMP, 'HH'), 'DD-MON-YYYY HH24:MI:SS')ASC;